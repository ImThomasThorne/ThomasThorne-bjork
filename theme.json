			"spacingSizes": [
				{
					"name": "1",
					"size": "8px",
					"slug": "10"
				},
				{
					"name": "2",
					"size": "12px",
					"slug": "20"
				},
				{
					"name": "3",
					"size": "16px",
					"slug": "30"
