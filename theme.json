    "styles": {
        "blocks": {
            "core/button": {
				"border": {
					"radius": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)"
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "1em"
				}
			},
			"core/image": {
				"border": {
					"radius": "1rem"
				}
			},
			"core/gallery": {
				"border": {
					"radius": "1rem"
				},
				"spacing": {
					"blockGap": "1rem"
				}
			},
			"core/query": {
				"spacing": {
					"blockGap": "1rem"
				}
			},
            "core/pullquote": {
				"border": {
					"bottom": {
						"style": "none",
						"width": "0px"
					},
					"color": "var(--wp--preset--color--primary)",
					"left": {
						"style": "none",
						"width": "0px"
					},
					"right": {
						"style": "none",
						"width": "0px"
					},
					"style": "solid",
					"top": {
						"style": "none",
						"width": "0px"
					},
					"width": "0"
				},
				"color": {
					"text": "var(--wp--preset--color--custom-poppy)"
				},
				"spacing": {
					"padding": {
						"bottom": "0",
						"left": "0",
						"right": "0",
						"top": "0"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-3)",
					"lineHeight": "var(--wp--custom--typography--line-height--headings--large)"
				}
			},
            "core/query-pagination": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/quote": {
				"border": {
					"left": {
						"color": "var(--wp--preset--color--tertiary)",
						"style": "solid",
						"width": "1px"
					}
				},
				"spacing": {
					"padding": {
						"left": "1.5em"
					}
				}
			},
            "core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--heading-2)",
					"fontStyle": "inherit",
					"fontWeight": "600",
					"lineHeight": "1",
					"textTransform": "inherit"
				}
			},
            "core/post-title": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none",
                            "fontSize": "var(--wp--preset--font-size--heading-6)"
						}
					}
				}
			},
            "core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none",
                            "fontSize": "var(--wp--preset--font-size--paragraph)"
						}
					}
				}
			}
        },
        "color": {
            "background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
        },
        "elements": {
			"button": {
				"border": {
					"radius": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--custom-poppy)",
					"text": "var(--wp--preset--color--background)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "600",
					"lineHeight": "1"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--custom-crayola-pink)"
				},
				"spacing": {
					"margin": {
						"bottom": "0",
						"top": ".75em"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "italic",
					"fontWeight": "400",
					"lineHeight": "1"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--heading-1)",
					"fontStyle": "normal",
					"fontWeight": "900",
					"lineHeight": "1",
					"textTransform": "uppercase"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--heading-2)",
					"fontStyle": "normal",
					"fontWeight": "800",
					"lineHeight": "1"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--heading-3)",
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--heading-4)",
					"fontStyle": "normal",
					"fontWeight": "600",
					"lineHeight": "1"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--heading-5)",
					"fontStyle": "normal",
					"fontWeight": "600",
					"lineHeight": "1"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-6)",
					"fontStyle": "normal",
					"fontWeight": "500",
					"lineHeight": "1"
				}
			},
			"heading": {
				"spacing": {
					"margin": {
						"bottom": "1em",
						"top": ".75em"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontWeight": "600",
					"letterSpacing": "var(--wp--custom--typography--letter-spacing--gigantic)"
				}
			},
			"link": {
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--custom-poppy)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "normal",
					"fontWeight": "600",
					"lineHeight": "1",
					"textDecoration": "none"
				}
			}
		},
		"spacing": {
			"blockGap": "min(16px, 3.2vw)",
			"padding": {
				"bottom": "var(--wp--preset--spacing--body-gutter)",
				"left": "var(--wp--preset--spacing--body-gutter)",
				"right": "var(--wp--preset--spacing--body-gutter)",
				"top": "var(--wp--preset--spacing--body-gutter)"
			}
		},
        "typography": {
			"fontFamily": "var(--wp--preset--font-family--poppins)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontStyle": "normal",
			"fontWeight": "400",
			"letterSpacing": "var(--wp--custom--typography--letter-spacing--body)",
			"lineHeight": "var(--wp--custom--typography--line-height--body)"
		}
    }
}