{
    "$schema": "https://schemas.wp.org/wp/6.3/theme.json",
    "version": 2,
    "customTemplates": [

    ],
    "settings": {
        "appearanceTools": true,
        "color": {
            "palette": [
                {
                    "color": "#111111",
                    "name": "Foreground",
                    "slug": "foreground"
                },
                {
                    "color": "#FFFFFF",
                    "name": "Background",
                    "slug": "background"
                },
                {
					"color": "#dd4146",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#ed5d72",
