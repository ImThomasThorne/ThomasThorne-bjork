				{
					"name": "5",
					"size": "32px",
					"slug": "50"
				},
				{
					"name": "6",
					"size": "clamp(32px, 7.5vw, 40px)",
					"slug": "60"
				},
				{
					"name": "7",
					"size": "clamp(48px, 9vw, 64px)",
					"slug": "70"
				},
				{
					"name": "8",
					"size": "clamp( 64px, 6.666vw, 96px )",
					"slug": "80"
				},
				{
					"name": "Body Margin",
					"size": "clamp( 32px, 5vw, 64px )",
					"slug": "body-gutter"
